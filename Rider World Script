	end
end)


Tab_1:Button("Check Levels (Click This Everytime You Changed Rider)", function()
	for i = 1, 80 do task.wait()
		local Level_Text = game:GetService("Players").LocalPlayer.PlayerGui.RidersGUI.LevelDescription.Text
		if string.match(Level_Text, "Current Level : "..i) ~= nil and _G.Auto_Fram == true then
			_G.Level = tonumber(i)
			wait(0.15)
			Discord_Gui:Notification("Current Level", "Your Current Level Is "..i.."?", "Okay")
		end
	end
end)


Tab_1:Toggle("Auto Fram Select", false, function(Function)
	if Function then
		_G.Auto_Fram_Select = true
		
		
		while _G.Auto_Fram_Select == true do task.wait()
			pcall(function()
				if game:GetService("Workspace").Lives:FindFirstChild(_G.Monster_Select) then
					for _, v in pairs(game:GetService("Workspace").Lives:GetChildren()) do
						if v.Name == _G.Monster_Select and v.Humanoid.Health > 0 and game:GetService("Players").LocalPlayer.PlayerGui.RidersGUI.Enabled == true and _G.Is_Near == false then
							repeat task.wait()
								if _G.Select_Fram_Mode == "Above" then
									game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, _G.Distance, 0) * CFrame.fromOrientation(300, 0, 0)
								elseif _G.Select_Fram_Mode == "Behide" then
									game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 0, _G.Distance) * CFrame.fromOrientation(0, 0, 0)
								elseif _G.Select_Fram_Mode == "Under (Safe)" then
									game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, _G.Distance - (_G.Distance * 2), 0) * CFrame.fromOrientation(900, 0, 0)
								end
								
								if not game:GetService("Players").LocalPlayer.Character:FindFirstChild("Attack") then
									game:GetService("Players").LocalPlayer.Character.Humanoid:EquipTool(game:GetService("Players").LocalPlayer.Backpack["Attack"])
								else
									game:GetService("ReplicatedStorage").Remote.Event.Action:FireServer({["Input"] = "Mouse1", ["LightAttack"] = true})
								end
								
								setfflag("HumanoidParallelRemoveNoPhysics", "False")
								setfflag("HumanoidParallelRemoveNoPhysicsNoSimulate2", "False")
								game:GetService("Players").LocalPlayer.Character.Humanoid:ChangeState(11)
								
								
								if _G.E == true then
									game:GetService("ReplicatedStorage").Remote.Event.Riders:FireServer({["Key"] = "E",["Skill"] = _G.E,["MouseData"] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame})
								end
								if _G.R == true then
									game:GetService("ReplicatedStorage").Remote.Event.Riders:FireServer({["Key"] = "R",["Skill"] = _G.R,["MouseData"] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame})
								end
								if _G.C == true then
									game:GetService("ReplicatedStorage").Remote.Event.Riders:FireServer({["Key"] = "C",["Skill"] = _G.C,["MouseData"] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame})
								end
								if _G.V == true then
									game:GetService("ReplicatedStorage").Remote.Event.Riders:FireServer({["Key"] = "V",["Skill"] = _G.V,["MouseData"] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame})
								end
							until _G.Auto_Fram_Select ~= true or v.Humanoid.Health == 0 or _G.Is_Near == true
						elseif v.Name == _G.Monster_Select and v.Humanoid.Health <= 0 and game:GetService("Players").LocalPlayer.PlayerGui.RidersGUI.Enabled == true then
