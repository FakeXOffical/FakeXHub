	_G.Player_Select_Is = Function
end)


Tab_3:Button("Refesh", function()
	Dropdown_4:Clear()
	for i = 1, #game:GetService("Players"):GetChildren() do task.wait()
		Dropdown_4:Add(game:GetService("Players"):GetChildren()[i].Name)
	end
end)


