Tab_5:Label("// Anti Afk //")


Tab_5:Toggle("Anti Afk", false, function(Function)
	if Function then
		_G.Anti_Afk = true

		while _G.Anti_Afk == true do wait(300) 
			pcall(function()
				game:GetService("VirtualUser"):CaptureController()
				game:GetService("VirtualUser"):Button1Down(Vector2.new(1280, 672))
			end)
		end
	else
		_G.Anti_Afk = false
	end
end)


Tab_5:Label("// Effect //")


Tab_5:Toggle("Delete Effect", false, function(Function)
	if Function then
		game:GetService("Workspace").EFX:Destroy()
	else
		local Effect = Instance.new("Folder", game.Workspace)
		Effect.Name = "EFX"
	end
end)


Tab_5:Label("// Name //")


Tab_5:Toggle("Cool Name", false, function(Function)
	if Function then
		_G.Auto_Hide_Name = true
		while _G.Auto_Hide_Name == true do wait()
			pcall(function()
				local Text_Random1 = {"!", "@", "#", "&", "%", "?"}
				local Random1 = math.random(1, #Text_Random1)
				
				local Text_Random2 = {"!", "@", "#", "&", "%", "?"}
				local Random2 = math.random(1, #Text_Random2)
				
				local Text_Random3 = {"!", "@", "#", "&", "%", "?"}
				local Random3 = math.random(1, #Text_Random3)
				
				local Text_Random4 = {"!", "@", "#", "&", "%", "?"}
				local Random4 = math.random(1, #Text_Random4)
				if _G.Auto_Hide_Name == true then
					game:GetService("Players").LocalPlayer.Character.NAME_GUI.Box.Header.Text = "Fake X User".." Lv."..Text_Random1[Random1]..Text_Random2[Random2]..Text_Random3[Random3]..Text_Random4[Random4]
				else
					game:GetService("Players").LocalPlayer.Character.NAME_GUI.Box.Header.Text = "Fake X Hub User"
				end
			end)
		end
	else
		_G.Auto_Hide_Name = false
		game:GetService("Players").LocalPlayer.Character.NAME_GUI.Box.Header.Text = "Fake X Hub User"
	end
end)


Tab_5:Label("// Character //")


Tab_5:Button("Reset Character", function()
	game:GetService("Players").LocalPlayer.Character.Humanoid.Health = 0
end)
