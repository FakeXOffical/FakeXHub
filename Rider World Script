local Discord_Gui = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/discord%20lib.txt")()

local Window = Discord_Gui:Window("Fake X Hub")

local Server = Window:Server("Rider World V 1.0.8", "")

local Tab_1 = Server:Channel("- Auto Fram")

local Tab_2 = Server:Channel("- Auto Dungeon")

local Tab_3 = Server:Channel("- Players")

local Tab_4 = Server:Channel("- Teleport")
