	game:GetService("Workspace").Camera.CameraSubject = game:GetService("Players")[_G.Player_Select_Is].Character.Humanoid
end)


Tab_3:Button("Unview", function()
	game:GetService("Workspace").Camera.CameraSubject = game:GetService("Players").LocalPlayer.Character.Humanoid
end)


