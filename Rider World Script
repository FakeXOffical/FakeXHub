NPC_Teleport_Tab = {}
Player_In_The_Game = {}


for _, v in pairs(game:GetService("Workspace").NPC:GetChildren()) do
	table.insert(NPC_Teleport_Tab, v.Name)
end


for _, v in pairs(game:GetService("Players"):GetChildren()) do
	table.insert(Player_In_The_Game, v.Name)
end


_G.Is_Near = false


game:GetService("Players").LocalPlayer.PlayerGui.RidersGUI.LevelDescription.Changed:Connect(function()
	for i = (_G.Level - 1), 80 do wait()
		local Level_Text = game:GetService("Players").LocalPlayer.PlayerGui.RidersGUI.LevelDescription.Text
		if string.match(Level_Text, "Current Level : "..i) ~= nil and _G.Auto_Fram == true then
			_G.Level = tonumber(i)
		end
	end
end)


game:GetService("Players").PlayerAdded:Connect(function(Plr)
	if _G.Player_Old ~= #game:GetService("Players"):GetChildren() then
		Dropdown_4:Add(Plr.Name)
	end
end)


