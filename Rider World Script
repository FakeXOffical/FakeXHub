Tab_1:Toggle("Auto Fram Level", false, function(Function)
	if Function then
		_G.Auto_Fram = true
		
		while _G.Auto_Fram == true do task.wait()
			pcall(function()
				Check_Levels()
				if game:GetService("Players").LocalPlayer.PlayerGui.RidersGUI.Enabled == true and _G.Is_Near == false then
					if game:GetService("Workspace").Lives:FindFirstChild(Monster) then
						for _, v in pairs(game:GetService("Workspace").Lives:GetChildren()) do
							if v.Name == Monster and v.Humanoid.Health > 0 then
								repeat task.wait()
									if _G.Select_Fram_Mode == "Above" then
										game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, _G.Distance, 0) * CFrame.fromOrientation(300, 0, 0)
									elseif _G.Select_Fram_Mode == "Behide" then
										game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 0, _G.Distance) * CFrame.fromOrientation(0, 0, 0)
									elseif _G.Select_Fram_Mode == "Under (Safe)" then
										game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, _G.Distance - (_G.Distance * 2), 0) * CFrame.fromOrientation(900, 0, 0)
									end
									
									if not game:GetService("Players").LocalPlayer.Character:FindFirstChild("Attack") then
										game:GetService("Players").LocalPlayer.Character.Humanoid:EquipTool(game:GetService("Players").LocalPlayer.Backpack["Attack"])
									else
										game:GetService("ReplicatedStorage").Remote.Event.Action:FireServer({["Input"] = "Mouse1", ["LightAttack"] = true})
									end
									
									setfflag("HumanoidParallelRemoveNoPhysics", "False")
									setfflag("HumanoidParallelRemoveNoPhysicsNoSimulate2", "False")
									game:GetService("Players").LocalPlayer.Character.Humanoid:ChangeState(11)
									
									
									if _G.E == true then
										game:GetService("ReplicatedStorage").Remote.Event.Riders:FireServer({["Key"] = "E",["Skill"] = _G.E,["MouseData"] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame})
									end
									if _G.R == true then
										game:GetService("ReplicatedStorage").Remote.Event.Riders:FireServer({["Key"] = "R",["Skill"] = _G.R,["MouseData"] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame})
									end
									if _G.C == true then
										game:GetService("ReplicatedStorage").Remote.Event.Riders:FireServer({["Key"] = "C",["Skill"] = _G.C,["MouseData"] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame})
									end
									if _G.V == true then
										game:GetService("ReplicatedStorage").Remote.Event.Riders:FireServer({["Key"] = "V",["Skill"] = _G.V,["MouseData"] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame})
									end
								until _G.Auto_Fram ~= true or v.Humanoid.Health == 0 or v.Name ~= Monster
							elseif v.Name == Monster and v.Humanoid.Health <= 0 then
								game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = Place * CFrame.new(0, -15, 0)
							
								setfflag("HumanoidParallelRemoveNoPhysics", "False")
								setfflag("HumanoidParallelRemoveNoPhysicsNoSimulate2", "False")
								game:GetService("Players").LocalPlayer.Character.Humanoid:ChangeState(11)
							end
						end
					else
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Place
					end
				end
			end)
		end
	else
		_G.Auto_Fram = false
	end
end)


Tab_1:Toggle("Kill Aura", false, function(Function)
	if Function then
		_G.Kill_Aura = true
		
		while _G.Kill_Aura == true do task.wait()
			pcall(function()
				if game:GetService("Players").LocalPlayer.PlayerGui.RidersGUI.Enabled == true then
					for _, v in pairs(game:GetService("Workspace").Lives:GetChildren()) do
						if (game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude <= 50 and v.Humanoid.Health > 0 and not game:GetService("Players"):FindFirstChild(v.Name) then
							repeat task.wait()
								if _G.Select_Fram_Mode == "Above" then
									game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, _G.Distance, 0) * CFrame.fromOrientation(300, 0, 0)
								elseif _G.Select_Fram_Mode == "Behide" then
									game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 0, _G.Distance) * CFrame.fromOrientation(0, 0, 0)
								elseif _G.Select_Fram_Mode == "Under (Safe)" then
									game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, _G.Distance - (_G.Distance * 2), 0) * CFrame.fromOrientation(900, 0, 0)
								end
								
								if not game:GetService("Players").LocalPlayer.Character:FindFirstChild("Attack") then
									game:GetService("Players").LocalPlayer.Character.Humanoid:EquipTool(game:GetService("Players").LocalPlayer.Backpack["Attack"])
								else
									game:GetService("ReplicatedStorage").Remote.Event.Action:FireServer({["Input"] = "Mouse1", ["LightAttack"] = true})
								end
								
								setfflag("HumanoidParallelRemoveNoPhysics", "False")
								setfflag("HumanoidParallelRemoveNoPhysicsNoSimulate2", "False")
								game:GetService("Players").LocalPlayer.Character.Humanoid:ChangeState(11)
								
								
								if _G.E == true then
									game:GetService("ReplicatedStorage").Remote.Event.Riders:FireServer({["Key"] = "E",["Skill"] = _G.E,["MouseData"] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame})
								end
								if _G.R == true then
									game:GetService("ReplicatedStorage").Remote.Event.Riders:FireServer({["Key"] = "R",["Skill"] = _G.R,["MouseData"] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame})
								end
								if _G.C == true then
									game:GetService("ReplicatedStorage").Remote.Event.Riders:FireServer({["Key"] = "C",["Skill"] = _G.C,["MouseData"] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame})
								end
								if _G.V == true then
									game:GetService("ReplicatedStorage").Remote.Event.Riders:FireServer({["Key"] = "V",["Skill"] = _G.V,["MouseData"] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame})
								end
							until _G.Kill_Aura ~= true or v.Humanoid.Health == 0
						end
					end
				end
			end)
		end
	else
		_G.Kill_Aura = false
	end
end)


Tab_1:Toggle("Safe Mode", false, function(Function)
	if Function then
		_G.Position = true


		while _G.Position == true do wait(0.5)
			pcall(function()
				for _, v in pairs(game:GetService("Players"):GetChildren()) do
					if v.Name ~= game:GetService("Players").LocalPlayer.Name then
						if (v.Character.HumanoidRootPart.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < 75 then
							
							_G.Is_Near = true
							
							local Old_Position = v.Character.HumanoidRootPart.Position
							print(v.Name, (Old_Position - v.Character.HumanoidRootPart.Position).Magnitude)
							repeat task.wait()
								game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame * CFrame.new(0, 3000, 0)
								setfflag("HumanoidParallelRemoveNoPhysics", "False")
								setfflag("HumanoidParallelRemoveNoPhysicsNoSimulate2", "False")
								game:GetService("Players").LocalPlayer.Character.Humanoid:ChangeState(11)
							until (Old_Position - v.Character.HumanoidRootPart.Position).Magnitude >= 20 or _G.Position ~= true
						else
							_G.Is_Near = false
						end
					end
				end
			end)
		end
	else
		_G.Position = false
	end
end)


